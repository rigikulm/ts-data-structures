{
    "globals": {
        "expect": true,
        "it": true,
        "describe": true,
        "beforeEach": true,
        "afterEach": true,
        "document": false,
        "navigator": false,
        "window": false
      },
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "class-methods-use-this": 0,
        "no-plusplus": 0,
        "arrow-parens": 0,
        "no-console": 0,
        "import/extensions": 0,
        "import/no-unresolved": 0,
        "import/prefer-default-export": 0,
        "comma-dangle": 0,
        "no-underscore-dangle": 0,
        "no-param-reassign": 0,
        "no-return-assign": 0,
        "no-restricted-globals": 0,
        "no-multi-assign": 0,
        "prefer-destructuring": ["error", {"object": true, "array": false}]
      }
}
